{
  "name": "nib-underground-provider-portal",
  "version": "0.0.1",
  "description": "Provider Portal for nib NZ",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests tests_dom",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:bamboo": "better-npm-run test -R mocha-bamboo-reporter",
    "test:dev": "npm run test -- --watch",
    "dom-test": "./node_modules/.bin/testcafe --color electron:tests_dom/test_utils/electron_browser.js tests_dom/*.spec.js",
    "e2e-test": "./node_modules/.bin/testcafe --color electron:tests_dom/test_utils/electron_browser.js tests_dom/tests_e2e/**/*.spec.js",
    "e2e-browser-test": "./node_modules/.bin/testcafe --color chrome,firefox,ie,edge  tests_dom/tests_e2e/**/*.spec.js",
    "e2e-chrome-test": "./node_modules/.bin/testcafe --color chrome tests_dom/tests_e2e/**/*.spec.js",
    "karma-test": "better-npm-run karma-test",
    "karma-test:dev": "npm run karma-test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "coverage": "nyc better-npm-run test"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "__TODO__command": "npm run lint && npm run test && npm run clean && npm run compile",
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/mocha/bin/mocha --require ./tests/test_helper.js --compilers js:babel-core/register \"./tests/**/*.spec.js\"",
      "env": {
        "NODE_ENV": "test",
        "NODE_PATH": "src",
        "DEBUG": "app:*"
      }
    },
    "karma-test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "nyc": {
    "exclude": [
      "bin",
      "coverage",
      "dist",
      "server",
      "tests",
      "tests_dom",
      "**/*.config.js"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/nib-nz/nib.undergound"
  },
  "author": "nib NZ, Solnet Solutions Ltd",
  "license": "UNLICENSED",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.23.0",
